    "vmSizeSelect": {
      "value": "Standard_A3"
    },
    "location": {
      "value": "#location#"
    },
    "_artifactsLocation": {
      "value": "https://raw.githubusercontent.com/wls-eng/arm-oraclelinux-wls/master/src/main/"
